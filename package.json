{
  "name": "trackmasters",
  "version": "0.1.0",
  "private": true,
  "author": "Nikolas Charalambidis",
  "license": "MIT",
  "homepage": "http://nikolas-charalambidis.github.io/trackmasters.github.io/",
  "scripts": {
    "analyze:prod": "webpack --env.ENVIRONMENT=production --env.ANALYZE",
    "lint": "eslint --max-warnings 0 --ext .js \"src/**/*.{js,jsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,scss,sass}\"",
    "prettier:check": "prettier --list-different \"src/**/*.{js,jsx,scss,sass}\"",
    "dev": "webpack-dev-server --env.ENVIRONMENT=development --open",
    "build:dev": "webpack --env.ENVIRONMENT=development",
    "build": "webpack -p --env.ENVIRONMENT=production",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-dev-utils": "^11.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.2.2",
    "react-scripts": "3.1.2",
    "remark-gfm": "^1.0.0",
    "uikit": "^3.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-react-inline-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "prettier": {
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn fmt:check"
    }
  }
}
